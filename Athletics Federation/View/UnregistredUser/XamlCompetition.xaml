<Window x:Class="Athletics_Federation.View.UnregistredUser.XamlCompetition"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Athletics_Federation.View.UnregistredUser"
        mc:Ignorable="d"
        Title="Компетенции" Height="219.952" Width="260" Loaded="Window_Loaded">
    <Grid>
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="Black" Offset="0"/>
                <GradientStop Color="#FF301A7A" Offset="1"/>
            </LinearGradientBrush>
        </Grid.Background>
        <DataGrid x:Name="DGridCompetition" AutoGenerateColumns="False" IsReadOnly="True" Margin="10,10,10,0" Background="{x:Null}" Height="169" VerticalAlignment="Top">
            <DataGrid.Columns>
                <DataGridTextColumn Binding="{Binding NameCompetition}" Header="                     Компетенция" Width="*"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
